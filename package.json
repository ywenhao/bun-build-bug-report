{
  "name": "bun-demo",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch index.ts",
    "start-js": "bun run ./dist/index.js",
    "start-win": "bun run ./dist/index.exe",
    "start-linux": "bun run ./dist/index",
    "build-js": "bun build --outdir ./dist ./index.ts",
    "build-win": "bun build --compile --target=bun-win-x64 --outfile ./dist/index.exe ./index.ts",
    "build-linux": "bun build --compile --target=bun-linux-x64 --outfile ./dist/index ./index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@alicloud/ocr-api20210707": "^3.1.3"
  }
}
